{
  "Python File Header": {
    "prefix": "head",
    "body": [
      "\"\"\"",
      "Description: ${1}",
      "Author: ${2:Francisco Aar√≥n Ortega Anguiano <aaron.ortega15@outlook.es>}",
      "Created: ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE} ${CURRENT_HOUR}:${CURRENT_MINUTE}:${CURRENT_SECOND}",
      "\"\"\"",
      "$0"
    ],
    "description": "Python source file header"
  },
  "Python Docstring": {
    "prefix": "docs",
    "body": [
      "\"\"\"${1:Brief description}",
      "",
      "Args:",
      "    ${2:param: Description}",
      "",
      "Returns:",
      "    ${3:Description of return value}",
      "\"\"\"",
      "$0"
    ],
    "description": "Python docstring"
  },
  "Print Debug": {
    "prefix": "print",
    "body": "print(f\"the value of ${1} is {${2}}\")",
    "description": "Print value of some variable"
  },
  "Import Alias": {
    "prefix": "impa",
    "body": "import ${1:FOO} as ${2:BAR}",
    "description": "import FOO as BAR"
  },
  "Main Function": {
    "prefix": "main",
    "body": [
      "def main():",
      "    ${0}",
      "",
      "if __name__ == \"__main__\":",
      "    main()"
    ],
    "description": "Main function boilerplate"
  },
  "Solution": {
    "prefix": "sol",
    "body": [
      "solution = Solution()",
      "print(solution.${0})"
    ],
    "description": "solution"
  },
  "Printf": {
    "prefix": "printf",
    "body": [
      "print(f'${0}')"
    ],
    "description": "solution"
  }
}
