{
  "JavaScript File Header": {
    "prefix": "head",
    "body": [
      "/**",
      " * Description: ${1:File description}",
      " * Author: ${2:Francisco Aar√≥n Ortega Anguiano <aaron.ortega15@outlook.es>}",
      " * Created: ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE} ${CURRENT_HOUR}:${CURRENT_MINUTE}:${CURRENT_SECOND}",
      " */",
      "",
      "$0"
    ],
    "description": "JavaScript file header"
  },
  "Console Log": {
    "prefix": "clg",
    "body": "console.log(\"${1:label}: \", ${2:value});",
    "description": "Console log value"
  },
  "Arrow Function": {
    "prefix": "afn",
    "body": [
      "const ${1:functionName} = (${2:params}) => {",
      "  ${0}",
      "};"
    ],
    "description": "Arrow function"
  }
}
